<form action="/sign-up" method="POST">
  <div class="form_group_container">
    <div class="form_group">
      <label for="username">Username</label>
      <input id="username" name="username" placeholder="Username" type="text" required>
    </div>
    <div class="form_group">
      <label  for="password">Password</label>
      <input id="password" name="password" placeholder="Password" type="text" required>
    </div>
  </div>
  <div class="form_group_container">
    <div class="form_group">
      <label  for="fname">First Name</label>
      <input id="fname" name="fname" placeholder="First" type="text" required>
    </div>
    <div class="form_group">
      <label  for="lname">Last Name</label>
      <input id="lname" name="lname" placeholder="Last" type="text" required>
    </div>
  </div>
  <button>Sign Up</button>
</form>